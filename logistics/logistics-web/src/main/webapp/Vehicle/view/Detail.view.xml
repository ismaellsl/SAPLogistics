<mvc:View 
    controllerName="Vehicle.view.Detail"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form">
    <Page
        id="detailPage" 
        navButtonPress="onNavBack" 
        title="{i18n>detailTitle}" 
        showNavButton="{device>/isPhone}">
        <content>
            <ObjectHeader 
                id="detailHeader"				title="{VehicleInscription}"				number="{Id}"				numberUnit=""                introActive="false" 
                titleActive="false" 
                iconActive="false">
                <attributes
                    id="detailAttributes">                
                    <ObjectAttribute 
                        id="attribute" 
                        text="{i18n>detailText}" 
                        active="false">
                    </ObjectAttribute>
                </attributes>
                <firstStatus
                    id="detailStatus">
                    <ObjectStatus 
                        id="status"						text="{Plate}">
                    </ObjectStatus>
                </firstStatus>
            </ObjectHeader>
            <IconTabBar 
                id="idIconTabBar" 
                expanded="{device>/isNoPhone}">
    			<items 
    			    id="detailsItems">
                    <IconTabFilter 
                        id="iconTabFilter1" 
                        key="selfInfo" 
                        icon="sap-icon://hint">
                        <content>
							<f:SimpleForm 
								id="iconTabFilter1form" 
								minWidth="1024" 
								editable="false" 
								layout="ResponsiveGridLayout" 
								labelSpanL="3" 
								labelSpanM="3" 
								emptySpanL="4" 
								emptySpanM="4" 
								columnsL="1">
								<f:content>
									<Label 
										id="label1" 
										text="Capacity">
									</Label>
									<Text 
										id="text1"										text="{Capacity}"										maxLines="0">
									</Text>
									<Label 
										id="label2" 
										text="Chassi">
									</Label>
									<Text 
										id="text2"										text="{Chassi}"										maxLines="0">
									</Text>
									<Label 
										id="label3" 
										text="FiscalNumber">
									</Label>
									<Text 
										id="text3"										text="{FiscalNumber}"										maxLines="0">
									</Text>
									<core:ExtensionPoint 
										name="extIconTabFilterForm1"/>
								</f:content>
							</f:SimpleForm>
                        </content>
                    </IconTabFilter>
                    <IconTabFilter 
                        id="iconTabFilter2" 
                        key="" 
                        icon="sap-icon://enter-more">
                        <content>
							<f:SimpleForm 
								id="iconTabFilter2form" 
								minWidth="1024" 
								editable="false" 
								layout="ResponsiveGridLayout" 
								labelSpanL="3" 
								labelSpanM="3" 
								emptySpanL="4" 
								emptySpanM="4" 
								columnsL="1">
								<f:content>
									<Label 
										id="label4" 
										text="">
									</Label>
									<Text 
										id="text4"										text=""										maxLines="0">
									</Text>
									<Label 
										id="label5" 
										text="">
									</Label>
									<Text 
										id="text5"										text=""										maxLines="0">
									</Text>
									<Label 
										id="label6" 
										text="">
									</Label>
									<Text 
										id="text6"										text=""										maxLines="0">
									</Text>
									<core:ExtensionPoint 
										name="extIconTabFilterForm2"/>
								</f:content>
							</f:SimpleForm>
                        </content>
                    </IconTabFilter>
    				<core:ExtensionPoint
    				    name="extIconTabFilter"/>
                </items>
            </IconTabBar>
    		<core:ExtensionPoint name="extDetail"/>
        </content>
        <footer
            id="detailFooter">
             <OverflowToolbar id="otbFooter">
             <ToolbarSpacer/>
                <OverflowToolbarButton text="Delete" icon="sap-icon://delete" press="deleteVehicle"/>
                <OverflowToolbarButton text="Edit" icon="sap-icon://edit"/>
                <OverflowToolbarButton text="Add" icon="sap-icon://add"  />
             </OverflowToolbar>
        </footer>
    </Page>
</mvc:View>