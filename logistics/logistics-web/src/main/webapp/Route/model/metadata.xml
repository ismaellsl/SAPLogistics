<?xml version='1.0' encoding='UTF-8'?>
<edmx:Edmx Version="1.0"
    xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx">
    <edmx:DataServices m:DataServiceVersion="1.0"
        xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata">
        <Schema Namespace="logistics"
            xmlns="http://schemas.microsoft.com/ado/2008/09/edm">
            <EntityType Name="Passenger">
                <Key>
                    <PropertyRef Name="Id"/>
                </Key>
                <Property Name="Email" Type="Edm.String"/>
                <Property Name="Id" Type="Edm.Int32" Nullable="false"/>
                <Property Name="MobileNumber" Type="Edm.Int64"/>
                <Property Name="Name" Type="Edm.String"/>
                <Property Name="Registration" Type="Edm.String"/>
                <Property Name="Route" Type="Edm.Int32"/>
                <NavigationProperty Name="RouteDetails" Relationship="logistics.Passenger_Route" FromRole="Passenger" ToRole="Route"/>
            </EntityType>
            <EntityType Name="Driver">
                <Key>
                    <PropertyRef Name="Id"/>
                </Key>
                <Property Name="Id" Type="Edm.Int32" Nullable="false"/>
                <Property Name="Name" Type="Edm.String"/>
                <Property Name="Picture" Type="Edm.String"/>
                <Property Name="Registration" Type="Edm.String"/>
                <NavigationProperty Name="TripDetails" Relationship="logistics.Driver_Trip" FromRole="Driver" ToRole="Trip"/>
            </EntityType>
            <EntityType Name="Route">
                <Key>
                    <PropertyRef Name="Id"/>
                </Key>
                <Property Name="ArrivalLocation" Type="Edm.String"/>
                <Property Name="ArrivalTime" Type="Edm.DateTime"/>
                <Property Name="DepartureTime" Type="Edm.DateTime"/>
                <Property Name="DepertureLocation" Type="Edm.String"/>
                <Property Name="Distance" Type="Edm.Int64"/>
                <Property Name="Id" Type="Edm.Int32" Nullable="false"/>
                <NavigationProperty Name="PassengerDetails" Relationship="logistics.Passenger_Route" FromRole="Route" ToRole="Passenger"/>
                <NavigationProperty Name="TripDetails" Relationship="logistics.Route_Trip" FromRole="Route" ToRole="Trip"/>
            </EntityType>
            <EntityType Name="TripData">
                <Key>
                    <PropertyRef Name="Id"/>
                </Key>
                <Property Name="DateTime" Type="Edm.DateTime"/>
                <Property Name="EngineRPM" Type="Edm.Int64"/>
                <Property Name="FuelLevel" Type="Edm.Int64"/>
                <Property Name="Id" Type="Edm.Int32" Nullable="false"/>
                <Property Name="Speed" Type="Edm.Int64"/>
                <Property Name="Trip" Type="Edm.Int32"/>
                <NavigationProperty Name="TripDetails" Relationship="logistics.Trip_TripData" FromRole="TripData" ToRole="Trip"/>
            </EntityType>
            <EntityType Name="Trip">
                <Key>
                    <PropertyRef Name="Id"/>
                </Key>
                <Property Name="ArrivalTime" Type="Edm.DateTime"/>
                <Property Name="DepartureTime" Type="Edm.DateTime"/>
                <Property Name="Distance" Type="Edm.Int64"/>
                <Property Name="Driver" Type="Edm.Int32"/>
                <Property Name="Id" Type="Edm.Int32" Nullable="false"/>
                <Property Name="PassengerNr" Type="Edm.Int32"/>
                <Property Name="Route" Type="Edm.Int32"/>
                <Property Name="Vehicle" Type="Edm.Int32"/>
                <NavigationProperty Name="DriverDetails" Relationship="logistics.Driver_Trip" FromRole="Trip" ToRole="Driver"/>
                <NavigationProperty Name="RouteDetails" Relationship="logistics.Route_Trip" FromRole="Trip" ToRole="Route"/>
                <NavigationProperty Name="TripDataDetails" Relationship="logistics.Trip_TripData" FromRole="Trip" ToRole="TripData"/>
                <NavigationProperty Name="VehicleDetails" Relationship="logistics.Trip_Vehicle" FromRole="Trip" ToRole="Vehicle"/>
            </EntityType>
            <EntityType Name="Vehicle">
                <Key>
                    <PropertyRef Name="Id"/>
                </Key>
                <Property Name="Capacity" Type="Edm.Int32"/>
                <Property Name="Chassi" Type="Edm.String"/>
                <Property Name="FiscalNumber" Type="Edm.String"/>
                <Property Name="FuelType" Type="Edm.String"/>
                <Property Name="Id" Type="Edm.Int32" Nullable="false"/>
                <Property Name="Model" Type="Edm.String"/>
                <Property Name="Plate" Type="Edm.String"/>
                <Property Name="VehicleInscription" Type="Edm.String"/>
                <Property Name="Year" Type="Edm.Int32"/>
                <NavigationProperty Name="TripDetails" Relationship="logistics.Trip_Vehicle" FromRole="Vehicle" ToRole="Trip"/>
            </EntityType>
            <Association Name="Passenger_Route">
                <End Type="logistics.Passenger" Multiplicity="*" Role="Passenger"/>
                <End Type="logistics.Route" Multiplicity="1" Role="Route"/>
                <ReferentialConstraint>
                    <Principal Role="Route">
                        <PropertyRef Name="Id"/>
                    </Principal>
                    <Dependent Role="Passenger">
                        <PropertyRef Name="Route"/>
                    </Dependent>
                </ReferentialConstraint>
            </Association>
            <Association Name="Trip_TripData">
                <End Type="logistics.TripData" Multiplicity="*" Role="TripData"/>
                <End Type="logistics.Trip" Multiplicity="1" Role="Trip"/>
                <ReferentialConstraint>
                    <Principal Role="Trip">
                        <PropertyRef Name="Id"/>
                    </Principal>
                    <Dependent Role="TripData">
                        <PropertyRef Name="Trip"/>
                    </Dependent>
                </ReferentialConstraint>
            </Association>
            <Association Name="Driver_Trip">
                <End Type="logistics.Driver" Multiplicity="1" Role="Driver"/>
                <End Type="logistics.Trip" Multiplicity="*" Role="Trip"/>
                <ReferentialConstraint>
                    <Principal Role="Driver">
                        <PropertyRef Name="Id"/>
                    </Principal>
                    <Dependent Role="Trip">
                        <PropertyRef Name="Driver"/>
                    </Dependent>
                </ReferentialConstraint>
            </Association>
            <Association Name="Route_Trip">
                <End Type="logistics.Route" Multiplicity="1" Role="Route"/>
                <End Type="logistics.Trip" Multiplicity="*" Role="Trip"/>
                <ReferentialConstraint>
                    <Principal Role="Route">
                        <PropertyRef Name="Id"/>
                    </Principal>
                    <Dependent Role="Trip">
                        <PropertyRef Name="Route"/>
                    </Dependent>
                </ReferentialConstraint>
            </Association>
            <Association Name="Trip_Vehicle">
                <End Type="logistics.Trip" Multiplicity="*" Role="Trip"/>
                <End Type="logistics.Vehicle" Multiplicity="1" Role="Vehicle"/>
                <ReferentialConstraint>
                    <Principal Role="Vehicle">
                        <PropertyRef Name="Id"/>
                    </Principal>
                    <Dependent Role="Trip">
                        <PropertyRef Name="Vehicle"/>
                    </Dependent>
                </ReferentialConstraint>
            </Association>
            <EntityContainer Name="logisticsContainer" m:IsDefaultEntityContainer="true">
                <EntitySet Name="Passengers" EntityType="logistics.Passenger"/>
                <EntitySet Name="Drivers" EntityType="logistics.Driver"/>
                <EntitySet Name="Routes" EntityType="logistics.Route"/>
                <EntitySet Name="TripDatas" EntityType="logistics.TripData"/>
                <EntitySet Name="Trips" EntityType="logistics.Trip"/>
                <EntitySet Name="Vehicles" EntityType="logistics.Vehicle"/>
                <AssociationSet Name="Passenger_RouteSet" Association="logistics.Passenger_Route">
                    <End EntitySet="Passengers" Role="Passenger"/>
                    <End EntitySet="Routes" Role="Route"/>
                </AssociationSet>
                <AssociationSet Name="Trip_TripDataSet" Association="logistics.Trip_TripData">
                    <End EntitySet="TripDatas" Role="TripData"/>
                    <End EntitySet="Trips" Role="Trip"/>
                </AssociationSet>
                <AssociationSet Name="Driver_TripSet" Association="logistics.Driver_Trip">
                    <End EntitySet="Drivers" Role="Driver"/>
                    <End EntitySet="Trips" Role="Trip"/>
                </AssociationSet>
                <AssociationSet Name="Route_TripSet" Association="logistics.Route_Trip">
                    <End EntitySet="Routes" Role="Route"/>
                    <End EntitySet="Trips" Role="Trip"/>
                </AssociationSet>
                <AssociationSet Name="Trip_VehicleSet" Association="logistics.Trip_Vehicle">
                    <End EntitySet="Trips" Role="Trip"/>
                    <End EntitySet="Vehicles" Role="Vehicle"/>
                </AssociationSet>
            </EntityContainer>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>